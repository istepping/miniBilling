.schoolhome{
  width: 100%;
  height: 600rpx;
  border-bottom: 2px solid #F5F5F5;
}
.icon{
  display: flex;
  justify-content: center;
  align-items: center;
}
.name{
  text-align: center;
}
.kaiqi{
  height: 100rpx;
  display: flex;
  align-items: center;
  border-bottom: 2px solid #F5F5F5;
}
.text{
  width: 80%;
  margin-left: 10px;
}
.button{
  width: 30px;
  height: 30px;
}
.btn{
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(189, 189, 186, 0.74);
  color: #FFF;
  width: 60px;
  height: 30px;
}
.show{
  width: 100%;
  height: 40%;
  background-color: yellow;
}