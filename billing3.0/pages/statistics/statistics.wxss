.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 9000;
    opacity: 0.7;
}
 

.item{
  width:40%;
  height: 22%;
  background-color: white;
  margin: 20rpx;
  box-shadow: 3rpx 3rpx 3rpx 3rpx lightgray;
  border-radius: 10rpx;
}
.itemImage{
  width: 70rpx;
  height: 70rpx;
  margin: 20rpx;
}
.navagator{
  width:100%;
   display: flex;
  align-items: center;
}
